print("Guess the number (1-100):")


number = math.random(1,100)

while true do

guess = io.read()
guess = tonumber(guess) -- string to number

if guess == number then
  print("You Win!")
  break
  
elseif guess > number then
  print("to high")
elseif guess < number then
  print("to low")
 
end --END of IF

end --END of While loop
